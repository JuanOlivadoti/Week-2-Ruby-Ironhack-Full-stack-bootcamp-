<img src='foto.jpg'>

<h1> La fecha de hoy es <%= @time_now %> </h1>

<h1>Nuevo Twit</h1>

<form action='crear_twit' method='post'>
	<input type="text" name="user">
	<input type="text" name="password">
	<input type="submit">
</form>

<h2>Antiguos Twits</h2>
<ul>
	<% session[:twits].each do |twit| %>
		<li><%= twit %></li>
	<% end %>
</ul>
